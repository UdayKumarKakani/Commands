pipeline {
 	agent any
 	stages {
		stage('Checkout') {
 			steps {
 				echo "Code checkout"
 			}
 		}
 		stage('Build') {
 			steps {
 				echo "Build"
 			}
 		}
 		stage('Test') {
 			steps {
 				echo "Test"
 			}
 		}
		stage('IntegrationTest') {
 			steps {
 				echo "Intergation Test"
 			}
 		}
 	}
}
